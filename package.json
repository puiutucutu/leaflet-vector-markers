{
  "name": "leaflet-vector-markers",
  "version": "0.0.6",
  "description": "Vector SVG markers for Leaflet, with an option for Font Awesome/Twitter Bootstrap icons.",
  "main": "dist/leaflet-vector-markers.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/puiutucutu/leaflet-vector-markers"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production BABEL_ENV=build npm run build:node & npm run build:dist",
    "build:node": "cross-env NODE_ENV=production BABEL_ENV=build webpack --progress --config ./webpack/webpack.node.js",
    "build:dist": "cross-env NODE_ENV=production BABEL_ENV=build webpack --progress --config ./webpack/webpack.base.js",
    "start": "cross-env NODE_ENV=development BABEL_ENV=build webpack-dev-server --config ./webpack/webpack.development.js",
    "test:unit": "cross-env BABEL_ENV=test riteway -r @babel/register ./test/**/*.test.js | tap-spec",
    "test": "npm run test:unit",
    "test:watch": "nodemon --watch src --watch test -x \"npm run test\"",
    "coverage": "cross-env BABEL_ENV=test nyc riteway ./test/**/*.js | tap-spec"
  },
  "dependencies": {
    "leaflet": "^1.4.0"
  },
  "peerDependencies": {
    "leaflet": "^1.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.4.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-istanbul": "^5.1.3",
    "clean-webpack-plugin": "^2.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.10",
    "documentation": "^10.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^15.0.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.11",
    "nyc": "^14.0.0",
    "postcss": "^7.0.14",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "riteway": "^6.0.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.0",
    "tap-spec": "^5.0.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "keywords": [
    "Leaflet",
    "Markers",
    "Map",
    "SVG",
    "Vector",
    "Icon"
  ]
}
